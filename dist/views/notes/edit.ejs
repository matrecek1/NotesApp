<% layout('/layout/boilerplate') -%>
<form action="/notes/<%= note._id %>?_method=PUT" method="post">
    <div class="mb-3">
        <label for="form-title" class="form-label">Title</label>
        <input type="text" value="<%= note.title %> " name="note[title]" id="form-title" class="form-control">
    </div>
    <div class="mb-3">
        <label for="form-body" class="form-label">Title</label>
        <input type="text" value="<%= note.noteBody %> " name="note[noteBody]" id="form-body" class="form-control">
    </div>
    <button class="btn btn-primary" type="submit">Submit</button>
</form>